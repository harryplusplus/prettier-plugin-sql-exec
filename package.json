{
  "name": "prettier-plugin-sql-exec",
  "version": "0.1.0",
  "description": "A Prettier plugin that formats SQL by executing a formatting command.",
  "keywords": [
    "sql",
    "format",
    "prettier"
  ],
  "license": "MIT",
  "author": {
    "name": "harryplusplus",
    "email": "harryplusplus@gmail.com",
    "url": "https://harryplusplus.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test:integration": "vitest --run tests"
  },
  "devDependencies": {
    "@types/node": "24.9.1",
    "prettier": "3.6.2",
    "sort-package-json": "3.4.0",
    "tsup": "8.5.0",
    "typescript": "5.9.3",
    "vitest": "4.0.4"
  },
  "packageManager": "pnpm@10.15.1",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "onSuccess": "tsc --emitDeclarationOnly --declaration"
  }
}
